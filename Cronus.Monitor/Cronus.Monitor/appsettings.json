{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Cronus": {
    "BoundedContext": "cronus",
    "Tenants": [ "cronus" ],
    "Transport": {
      "RabbitMQ": {
        "Server": "docker-local.com",
        "VHost": "unicom-monitor"
      },
      "PublicRabbitMQ": {
        "Server": "docker-local.com",
        "VHost": "unicom-public"
      }
    }
  }
}