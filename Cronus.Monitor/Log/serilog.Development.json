{
  "Serilog": {
    "MinimumLevel": {
      // Verbose | Debug | Information | Warning | Error | Fatal
      "Default": "Debug",
      "Override": {
        "Elders": "Information",
        "Elders.Cronus": "Debug",
        "Microsoft": "Debug",
        "Microsoft.AspNetCore": "Debug",
        "System": "Debug"
      }
    },
    "WriteTo": [
      { "Name": "Debug" },
      {
        "Name": "Console",
        "Args": {
          "theme": "Log.EldersThemes::Epic, Log"
        }
      }
    ]
  }
}